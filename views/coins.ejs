
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.css">



<div class="ui inverted top attached demo menu">
  <a class="item">
    <i class="sidebar icon"></i>
  </a>
  <a class="item" href="/">
    COINLY
  </a>
</div>
<div class="ui bottom attached segment pushable">
  <div class="ui inverted labeled icon left inline vertical sidebar menu" style="">
    <a class="item">
      <i class="home icon"></i>
      Home
    </a>
    <a class="item">
      <i class="block layout icon"></i>
      Topics
    </a>
    <a class="item">
      <i class="smile icon"></i>
      Friends
    </a>
    <a class="item">
      <i class="calendar icon"></i>
      History
    </a>
  </div>
  <div class="pusher">
    <div class="ui basic segment">


<h2 align="center"> WELCOME TO RESULTS PAGE</h2>



<div class="ui sixteen wide centered grid">
  
  <div class="two wide column"></div>
  <div class="twelve wide column">
    
  <div class="ui search right align">
    <div class="ui icon input">
      <input class="prompt" type="text" placeholder="Search Coin...">
      <i class="search icon"></i>
    </div>
    <div class="results"></div>
  </div>
    <div class="ui containder">
        <table class="ui selectable striped celled grey very compact structured table">
          <thead>
            
            <tr>
              <th colspan="2">Project</th>
              <th colspan="3">ICO Price</th>
              <th colspan="3">Market Price</th>
              <th colspan="3">ICO Return</th>
            </tr>            
            <tr>
                <th>Project</th>
                <th >Symbol</th>
                <th>BTC ICO Price</th>
                <th>ETH ICO Price</th>
                <th>USD ICO Price</th>
                <th>BTC Current Price</th>
                <th>ETH Current Price</th>
                <th>USD Current Price</th>
                <th>BTC Return</th>
                <th>ETH Return</th>
                <th>USD Return</th>
                
            </tr>
            </thead>
            <tbody>
        <% coinDetails.forEach(function(coin){ %>    
          
                <tr>
                    <td ><%= coin.coinName %></td>
                    <td ><%= coin.coinSym %></td>
                    <td><%= coin.icobtcValue %></td>
                    <td><%= coin.icoethValue %></td>
                    <td><%= coin.icousdValue %></td>
                    <td><%= coin.btcValue %></td>
                    <td><%= coin.ethValue %> </td>
                    <td><%= coin.usdValue %> </td>
                    <td><%= (coin.btcValue/coin.icobtcValue).toFixed(2) %>X </td>
                    <td><%= (coin.ethValue/coin.icoethValue).toFixed(2) %>X </td>
                    <td><%= (coin.usdValue/coin.icousdValue).toFixed(2) %>X </td>
                </tr>
        
        <%})%>
            </tbody>
        </table>
    
    <a href="/">Search Again</a>
    </div>
  </div>
  <div class="two wide column"></div>
  
  
    
</div>


    </div>
  </div>
</div>



<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0/jquery.min.js"> </script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.4/semantic.js" type="text/javascript"></script>

<script type="text/javascript">
    // using context
    $('.ui.sidebar').sidebar({
        context: $('.bottom.segment')
      })
    .sidebar('attach events', '.menu .item');
</script>


